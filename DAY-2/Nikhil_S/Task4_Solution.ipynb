    "def gen_fib():\n",
    "    a = 0\n",
    "    b = 1\n",
    "    while True:\n",
    "        yield a\n",
    "        a,b = b, a + b\n",
    "\n",
    "fib = gen_fib()\n",
    "print(next(fib))\n",
    "\n",
    "for i in range(11):\n",
    "    print(next(fib))"
