    "def gen_fib(i):\n",
    "    \n",
    "    yield 0\n",
    "    yield 1\n",
    "    a = 0\n",
    "    b = 1\n",
    "    for i in range(1, i-2):\n",
    "        res = a + b\n",
    "        a = b\n",
    "        b = res\n",
    "        yield res\n",
    "        \n",
    "fib = gen_fib(10)\n",
    "\n",
    "for i in fib:\n",
    "    print(i)\n",
    "    \n",
    "    "
   ]
